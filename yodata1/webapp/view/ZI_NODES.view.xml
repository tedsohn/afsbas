<mvc:View
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    controllerName="yodata1.controller.main"
    displayBlock="true"
>
    <Page
        id="page"
        title="{i18n>title}"
    >
        <content>
            <Panel
                id="expandablePanel"
                expandable="true"
                headerText="YSO"
                expand="onExpand"
                width="auto"
                class="sapUiResponsiveMargin"
            >
                <List
                    id="_IDGenList1"
                    items="{/ZI_NODES}"
                >
                    <items>
                        <DisplayListItem
                            id="_IDGenDisplayListItem1"
                            value="{NodeKey}"
                            label="{NodeKey}"
                        />
                    </items>
                </List>
            </Panel>
            <Panel
                id="expandablePane2"
                expandable="true"
                headerText="YPRODUCT_C"
                width="auto"
                class="sapUiResponsiveMargin"
            >
                <List
                    id="_IDGenList2"
                    items="{prod>/ZI_NODES}"
                >
                    <items>
                        <DisplayListItem
                            id="_IDGenDisplayListItem2"
                            value="{prod>NodeKey}"
                            label="{prod>NodeKey}"
                        />
                    </items>
                </List>
            </Panel>
        </content>
    </Page>
</mvc:View>
