{
    "_version": "1.37.0",
    "sap.app": {
        "id": "zsmzgrcstatus",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.9.3",
            "toolsId": "9c64b74d-342c-4e5d-9678-df8b6f9f86b7"
        },
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/ZGRCSTATUS/",
